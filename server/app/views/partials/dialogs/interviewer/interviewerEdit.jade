md-dialog.interviewer(flex="40")
   md-toolbar
      h2.md-display-1(layout="row" layout-margin="auto") Edit Interviewer
   md-dialog-content
      form(name="interviewerEdit" novalidate)
         .inputs(layout="column")
            md-input-container
               label First Name
               input(type="text" name="interviewer.fName" ng-model="interviewer.fName")
            md-input-container
               label Last Name
               input(type="text" name="interviewer.lName" ng-model="interviewer.lName")
            md-input-container
               label EMail
               input(type="text" name="interviewer.email" ng-model="interviewer.email")
            md-input-container
               label Title
               input(type="text" name="interviewer.title" ng-model="interviewer.title")
            md-input-container
               label Company
               input(type="text" name="interviewer.company" ng-model="interviewer.company")
            md-input-container
               label Company Link
               input(type="text" name="interviewer.link" ng-model="interviewer.link")
            md-input-container
               label Company Description
               textarea(type="text" name="description" ng-model="interviewer.description")
            .loginbut(layout="row" layout-align="center center")
               md-button.md-raised(ng-click="edit(interviewer)") Save
               md-button.md-raised.md-warn(ng-click="close()") Cancel